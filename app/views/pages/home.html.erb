<% conversation = Conversation.joins(:user_conversations).where(user_conversations: {user_id: current_user.id}) %>
<% conversation.each do |s|%>
  <% if s.conversation_type == 'Single' %>
    <%= link_to s.user_conversations[1].user.full_name, message_path(s.conversation_id) %>
  <% end %>
<% end %>
